  a schema:Event;
  schema:name "Okanagan Fight Night 4";
  schema:description """
	<table class=\" BorderOn\">
		<tr>
			<td align=\"left\" colspan=\"1\"><img src=\"https://sysfilessacbe149174fee.blob.core.windows.net/public-container/clients/kelownacommunitytheatre/files/5299f843-223a-4e70-9354-2090d71320c7.jpeg\" /> </td><td align=\"left\"><h2>Victory International Promotion presents </h2><h1><span class=\"UnderlineText\">OKANAGAN FIGHT NIGHT 4</span> </h1></td>
		</tr>
