  schema:description """
	<table class=\" BorderOn\">
		<tr>
			<td align=\"left\" colspan=\"1\"><img src=\"https://sysfilessacbe149174fee.blob.core.windows.net/public-container/clients/kelownacommunitytheatre/files/59fe6453-840b-4729-957c-d68e1a29b11d.jpg\" /> </td><td align=\"left\"><h2>Dan Quinn Entertainment presents </h2><h1><span class=\"UnderlineText\">THE COMIC STRIPPERS (19+ only)</span></h1></td>
